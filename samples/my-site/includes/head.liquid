<style nonce>
/* taken from https://www.joshwcomeau.com/css/custom-css-reset/ */
*, *::before, *::after { box-sizing: border-box; }
* { margin: 0; }
body { line-height: 1.5; -webkit-font-smoothing: antialiased; }
img, picture, video, canvas, svg { display: block; max-width: 100%; }
input, button, textarea, select { font: inherit; }
p, h1, h2, h3, h4, h5, h6 { overflow-wrap: break-word; }
p { text-wrap: pretty; }
h1, h2, h3, h4, h5, h6 { text-wrap: balance; }
/* */
:root {
    --bg:#fff;
    --fc:#000;
    --w: 820px;
}
html { font-family: 'Trebuchet MS', Verdana, Geneva, Tahoma, sans-serif; }
body { background-color: var(--bg); color: var(--fc); }
main { max-width: var(--w); margin: 0 auto; }
p { margin: 1rem 0; }
code, pre.code { font-family: monaco, Consolas, LucidaConsole, monospace; }
/* csharp */
pre.background {
  background-color: #212121;
  color: #dfdfdf;
  margin: 0 -35px;
  padding: 15px 35px;
  border-radius: 15px;
}
.numeric { color: #b5cea8; }
.method { color: #dcdcaa; }
.class { color: #4ec9b0; }
.keyword { color: #569cd6; }
.string { color: #ce9178; }
.interface { color: #b8d7a3; }
.enumName { color: #b8d7a3; }
