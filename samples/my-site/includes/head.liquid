.comment { color: #6a9955; font-style: italic; }
.preprocessor { color: #808080; }
.preprocessorText { color: #a4a4a4; }
.struct { color: #86c691; }
.namespace { color: #dfdfdf; }
.enumMember { color: #dfdfdf; }
.identifier { color: #dfdfdf; }
.punctuation { color: #dfdfdf; }
.operator { color: #dfdfdf; }
.propertyName { color: #dfdfdf; }
.fieldName { color: #dfdfdf; }
.labelName { color: #dfdfdf; }
.operator_overloaded { color: #dfdfdf; }
